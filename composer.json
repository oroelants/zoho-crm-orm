{
  "name": "wabel/zoho-crm-orm",
  "description": "An ORM like package used to manipulate data from your Zoho CRM Account through the API.",
  "type": "library",
  "homepage": "http://mouf-php.com",
  "keywords": [
    "Zoho CRM",
    "ZohoCRM",
    "Zoho",
    "ORM"
  ],
  "require": {
    "php": ">=7.1",
    "wabel/zoho-crm-client-wrapper": "1.5.2",
    "gossi/php-code-generator": "^0.4 || ^0.5 || ^0.6",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "phpunit/php-code-coverage": "^6.1",
    "gossi/php-code-generator": "^0.6"
  },
  "autoload": {
    "psr-4": {
      "Wabel\\Zoho\\CRM\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wabel\\Zoho\\CRM\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/oroelants/zoho-crm-client-wrapper"
    },
    {
      "type": "git",
      "url": "https://github.com/oroelants/zcrm-php-sdk"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}